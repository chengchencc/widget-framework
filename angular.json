                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "example-ide:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/example-ide/src/test.ts",
                        "polyfills": "projects/example-ide/src/polyfills.ts",
                        "tsConfig": "projects/example-ide/tsconfig.spec.json",
                        "karmaConfig": "projects/example-ide/karma.conf.js",
                        "styles": [
                            "projects/example-ide/src/styles.scss"
                        ],
                        "scripts": [

                        ],
                        "assets": [
                            "projects/example-ide/src/favicon.ico",
                            "projects/example-ide/src/assets",
                            "node_modules/systemjs/dist/system.js"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/example-ide/tsconfig.app.json",
                            "projects/example-ide/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
